<Window x:Class="NTPServer.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NTPServer"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button Click="ButtonBase_OnClick" Content="RunScript" Height="50" Width="100"/>
        <Button x:Name="btnRunPS" Click="BtnRunPS_OnClick" Content="RunPS" HorizontalAlignment="Left" Height="50" Width="100"/>
        <TextBlock x:Name="txtResult" HorizontalAlignment="Left" VerticalAlignment="Top" Text="12" Width="800" Height="150"></TextBlock>
    </Grid>
</Window>
